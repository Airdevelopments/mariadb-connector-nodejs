{
  "globals": {
    "expect": true
  },
  "plugins": [
    "markdown"
  ],
  "parser":  "@typescript-eslint/parser",
  "extends": [
    "prettier",
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
     "es6": true
  },
  "rules": {
    "linebreak-style": ["error", "unix"]
  }
}
